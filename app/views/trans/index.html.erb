<script>
    $(document).ready(function () {
        $('select').material_select();
    });
</script>

<% @trans.each do |tran| %>
    <tr>
      <td><%= tran.amount %></td>
    </tr>
<% end %>



<div style="width: 100%;overflow:auto;">

  <div style="float:left; width: 70%">
    <% @trans.each do |tran| %>
        <table>
          <tbody>
          <tr>
            <td><%= render tran, account: Account.find_by(id: tran.account_id) %></td>
          </tr>
          </tbody>
        </table>
    <% end %>
  </div>


  <% @users_to_be_approved.each do |user| %>
      <tr>
        <td><%= user.updated_email %></td>
        <td><%= user.updated_phone %></td>
      </tr>
  <% end %>


  <div style="width: 100%;overflow:auto;">

    <div style="float:left; width: 70%">
      <% @users_to_be_approved.each do |user| %>
          <table>
            <tbody>
            <tr>
              <td><%= render user %></td>
            </tr>
            </tbody>
          </table>
      <% end %>
    </div>
  </div>

  <div style="float:right; width: 30%">
    <table class="highlight">
      <thead>
      <tr>
        <th> Manage users</th>
      </tr>
      </thead>

      <tbody>
      <% @user.each do |user| %>
          <tr>
            <td> <%= link_to "#{user.email}", user_accounts_path(user.id) %> </td>
          </tr>
      <% end %>

      </tbody>
    </table>
  </div>

</div>
